{
	"name": "svelte-virtual",
	"version": "1.0.0-next.12",
	"type": "module",
	"author": "Pedro AmÃ©rico",
	"license": "MIT",
	"repository": "github:ghostebony/svelte-virtual",
	"description": "Virtual List and Grid components for Svelte",
	"keywords": [
		"svelte",
		"virtual",
		"grid",
		"list",
		"scroll"
	],
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build": "vite build && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"packageManager": "pnpm@8.3.1",
	"engines": {
		"pnpm": ">=7"
	},
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.2",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"publint": "^0.1.11",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./grid": {
			"types": "./dist/grid/Grid.svelte.d.ts",
			"svelte": "./dist/grid/Grid.svelte",
			"default": "./dist/grid/Grid.svelte"
		},
		"./grid/Grid.svelte": {
			"types": "./dist/grid/Grid.svelte.d.ts",
			"svelte": "./dist/grid/Grid.svelte",
			"default": "./dist/grid/Grid.svelte"
		},
		"./list": {
			"types": "./dist/list/List.svelte.d.ts",
			"svelte": "./dist/list/List.svelte",
			"default": "./dist/list/List.svelte"
		},
		"./list/List.svelte": {
			"types": "./dist/list/List.svelte.d.ts",
			"svelte": "./dist/list/List.svelte",
			"default": "./dist/list/List.svelte"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		">4.0": {
			"grid": [
				"./dist/grid/Grid.svelte.d.ts"
			],
			"grid/Grid.svelte": [
				"./dist/grid/Grid.svelte.d.ts"
			],
			"list": [
				"./dist/list/List.svelte.d.ts"
			],
			"list/List.svelte": [
				"./dist/list/List.svelte.d.ts"
			],
			"types": [
				"./dist/types.d.ts"
			]
		}
	}
}
