{
	"name": "svelte-virtual",
	"version": "1.0.0-next.19",
	"type": "module",
	"author": "Pedro AmÃ©rico",
	"license": "MIT",
	"repository": "github:ghostebony/svelte-virtual",
	"description": "Virtual List and Grid components for Svelte",
	"keywords": [
		"svelte",
		"virtual",
		"grid",
		"list",
		"scroll"
	],
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build": "vite build && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"packageManager": "pnpm@9.12.3",
	"engines": {
		"pnpm": ">=7"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.12.0",
		"@typescript-eslint/parser": "^8.12.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"publint": "^0.2.12",
		"svelte": "^5.1.3",
		"svelte-check": "^4.0.5",
		"tslib": "^2.8.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.0",
		"vite": "^5.4.10"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./grid": {
			"types": "./dist/grid/Grid.svelte.d.ts",
			"svelte": "./dist/grid/Grid.svelte"
		},
		"./grid/Grid.svelte": {
			"types": "./dist/grid/Grid.svelte.d.ts",
			"svelte": "./dist/grid/Grid.svelte"
		},
		"./list": {
			"types": "./dist/list/List.svelte.d.ts",
			"svelte": "./dist/list/List.svelte"
		},
		"./list/List.svelte": {
			"types": "./dist/list/List.svelte.d.ts",
			"svelte": "./dist/list/List.svelte"
		},
		"./types": {
			"types": "./dist/types.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		">4.0": {
			"grid": [
				"./dist/grid/Grid.svelte.d.ts"
			],
			"grid/Grid.svelte": [
				"./dist/grid/Grid.svelte.d.ts"
			],
			"list": [
				"./dist/list/List.svelte.d.ts"
			],
			"list/List.svelte": [
				"./dist/list/List.svelte.d.ts"
			],
			"types": [
				"./dist/types.d.ts"
			]
		}
	}
}
