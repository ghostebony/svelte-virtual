{
	"useTabs": true,
	"singleQuote": false,
	"printWidth": 100,
	"tabWidth": 4,
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
